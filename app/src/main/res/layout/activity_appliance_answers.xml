<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_quiz"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context=".ApplianceUsage">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="711dp"
        android:layout_marginBottom="40dp"
        android:padding="8dp">

        <TextView
            android:id="@+id/name"
            android:layout_width="217dp"
            android:layout_height="231dp"
            android:layout_below="@+id/date"

            android:layout_alignParentLeft="true"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="9dp"
            android:layout_marginRight="9dp"
            android:layout_toStartOf="@+id/usage"
            android:layout_toLeftOf="@+id/usage"
            android:text="Score"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/usage"
            android:layout_width="151dp"
            android:layout_height="231dp"
            android:layout_below="@+id/date"

            android:layout_alignParentRight="true"
            android:layout_marginTop="7dp"
            android:layout_marginRight="20dp"
            android:text="0"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/date"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:text="Date?"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/name2"
            android:layout_width="182dp"
            android:layout_height="334dp"
            android:layout_below="@+id/name"
            android:layout_marginTop="61dp"
            android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp"
            android:layout_toStartOf="@+id/usage2"
            android:layout_toLeftOf="@+id/usage2"
            android:text="Name" />

        <TextView
            android:id="@+id/usage2"
            android:layout_width="159dp"
            android:layout_height="333dp"
            android:layout_below="@+id/usage"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            android:text="Usage" />

        <TextView
            android:id="@+id/date2"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/name"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="11dp"
            android:layout_marginLeft="11dp"
            android:layout_marginTop="14dp"
            android:text="Date2" />

    </RelativeLayout>




</LinearLayout>